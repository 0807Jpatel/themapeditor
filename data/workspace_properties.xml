<?xml version="1.0" encoding="UTF-8"?>
<!--
    This XML file allows us to easily customize the look and language used
    in our program's user interface. Note that this properties file is only
    for the workspace controls, which are custom for each application
-->
<properties>
    <property_list>
        <!-- PROGRAM ICON FILES -->
        <property name="ADD_ICON"               value="Add.png"></property>
        <property name="REMOVE_ICON"		value="Remove.png"></property>
        <property name="MOVE_UP_ICON"		value="MoveUp.png"></property>
        <property name="MOVE_DOWN_ICON"		value="MoveDown.png"></property>

            <!-- TOOLTIPS FOR BUTTONS -->
        <property name="ADD_ITEM_TOOLTIP"	    value="Select Background Color"></property>
        <property name="REMOVE_ITEM_TOOLTIP"		    value="Select Fill Color"></property>
        <property name="MOVE_UP_ITEM_TOOLTIP"	    value="Select Outline Color"></property>
        <property name="MOVE_DOWN_ITEM_TOOLTIP"	    value="Take Snapshot"></property>

        <!-- HEADINGS AND PROMPTS -->
        <property name="WORKSPACE_HEADING_LABEL"    value="To Do List"></property>
        <property name="DETAILS_HEADING_LABEL"	    value="Details"></property>
        <property name="NAME_PROMPT"                value="Name of Todo List:"></property>
        <property name="OWNER_PROMPT"               value="Owner:"></property>
        <property name="ITEMS_HEADING_LABEL"        value="Items"></property>

        <!-- TABLE COLUMN HEADINGS -->
        <property name="CATEGORY_COLUMN_HEADING"        value="Category"></property>
        <property name="DESCRIPTION_COLUMN_HEADING"     value="Description"></property>
        <property name="START_DATE_COLUMN_HEADING"      value="Start Date"></property>
        <property name="END_DATE_COLUMN_HEADING"        value="End Date"></property>
        <property name="COMPLETED_COLUMN_HEADING"       value="Completed"></property>


        <!-- ERROR MESSAGES AND TITLES FOR THEIR DIALOGS -->
        <property name="ATTRIBUTE_UPDATE_ERROR_MESSAGE"	    value="WARNING: An error occured updating the element's attribute."></property>
        <property name="ATTRIBUTE_UPDATE_ERROR_TITLE"	    value="Attribute Update Error"></property>
        <property name="UPDATE_ERROR_MESSAGE"		    value="An error occured updating the UI"></property>
        <property name="UPDATE_ERROR_TITLE"		    value="UI Update Error"></property>

        <!-- DIMENSION DIALOG TAGS -->
        <property name="DIMENSIONS_TITLE_LABEL"		    value="Dimension Dialog"></property>
        <property name="DIMENSION_HEIGHT_LABEL"		    value="Height: "></property>
        <property name="DIMENSION_WIDTH_LABEL"		    value="Width: "></property>

        <!-- NEW DIALOG TAGS -->
        <property name="NEW_TITLE_LABEL"		    value="New Dialog"></property>
        <property name="NEW_SUBNAME_LABEL"		    value="SubRegion Name:  "></property>
        <property name="NEW_DIRECTORY_LABEL"		value="Parent Region Directory: "></property>
        <property name="NEW_COORDINATE_LABEL"       value="Coordinates File: "></property>
        <property name="CHOOSE_FILE_BUTTON"         value="Choose"></property>
        <property name="NO_DIR_SELECTED"            value="  No Directory Selected"></property>
        <property name="NO_FILE_SELECTED"           value="  No File Selected"></property>

        <!-- OK AND CANCEL -->
        <property name="OK_BUTTON"		    value="Ok"></property>
        <property name="CANCEL_BUTTON"		    value="Cancel"></property>

        <!-- CSS PATHS -->
        <property name="DIALOG_CSS"		    value="css/dialog.css"></property>
        <property name="WORKSPACE_CSS"		    value="css/workspace.css"></property>



    </property_list>
    <property_options_list>
    </property_options_list>
</properties>